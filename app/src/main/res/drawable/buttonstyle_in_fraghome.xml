<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 평상시 -->
    <item android:state_pressed="false" android:state_focused="false">
        <shape>
            <solid android:color="#397493"/>
            <stroke android:color="#c4c2c2" android:width="1dp"/>
            <padding android:top="10dp" android:right="10dp" android:left="10dp" android:bottom="10dp"/>
        </shape>
    </item>
    <!-- 클릭했을 때 -->
    <item android:state_pressed="true" android:state_focused="false">
        <shape>
            <solid android:color="#f57d7b"/>
            <stroke android:color="#353535" android:width="3dp"/>
        </shape>
    </item>
</selector>